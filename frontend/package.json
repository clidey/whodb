    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "classnames": "^2.5.1",
    "codemirror": "^6.0.2",
    "framer-motion": "^12.23.13",
    "graphql": "^16.11.0",
