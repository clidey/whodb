    "@types/react-table": "^7.7.20",
    "classnames": "^2.5.1",
    "codemirror": "^6.0.1",
    "framer-motion": "^12.8.0",
    "graphql": "^16.11.0",
    "html-to-image": "^1.11.13",
    "lodash": "^4.17.21",
    "posthog-js": "^1.239.0",
