    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-table": "^7.7.20",
    "classnames": "^2.5.1",
    "codemirror": "^6.0.2",
    "framer-motion": "^12.23.12",
    "graphql": "^16.11.0",
    "html-to-image": "^1.11.13",
    "lodash": "^4.17.21",
    "posthog-js": "^1.257.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-json-view": "^1.21.3",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0",
