    "@vitejs/plugin-react": "4.7.0",
    "baseline-browser-mapping": "2.8.32",
    "nyc": "17.1.0",
    "typescript": "5.8.3",
    "vite": "7.1.11",
    "vite-plugin-istanbul": "7.2.1"
  },
