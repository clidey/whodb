    "@tailwindcss/postcss": "^4.1.4",
    "@types/lodash": "^4.17.16",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-istanbul": "^7.0.0",
    "buffer": "^6.0.3",
    "cypress": "^14.5.1",
    "nyc": "^17.1.0",
    "parcel": "^2.14.4",
    "postcss": "^8.5.3",
    "process": "^0.11.10",
    "tailwindcss": "^4.1.4"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
