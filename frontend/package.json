  "scripts": {
    "start": "vite --port 3000 --open",
    "start:ee": "VITE_BUILD_EDITION=ee vite --port 3000 --open",
    "build": "tsc -p tsconfig.ce.json && vite build",
    "build:ce": "tsc -p tsconfig.ce.json && vite build",
    "build:ee": "VITE_BUILD_EDITION=ee vite build",
    "# Primary E2E test commands": "",
    "e2e:ce": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh false all",
    "e2e:ce:headless": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh true all",
    "e2e:ce:headless:debug": "DEBUG=pw:api VITE_E2E_TEST=true bash ../dev/run-e2e.sh true all",
    "e2e:ee": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only false all",
    "e2e:ee:headless": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only true all",
    "e2e:all": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee false all",
    "e2e:all:headless": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee true all",
    "# Feature-specific commands - usage: pnpm e2e:feature <feature> (e.g., pnpm e2e:feature graph)": "",
    "# Features: chat, crud, data-types, data-view, explore, export, graph, key-types, keyboard-shortcuts, mock-data, pagination, query-history, scratchpad, search, settings, sorting, tables-list, type-casting, where-conditions": "",
    "e2e:feature": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh false all",
    "e2e:feature:headless": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh true all",
    "# EE feature-specific commands - usage: pnpm e2e:ee:feature <feature>": "",
    "e2e:ee:feature": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only false all",
    "e2e:ee:feature:headless": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only true all",
    "# CE database-specific commands - usage: pnpm e2e:db [database]": "",
    "# Available CE databases: postgres, mysql, mysql8, mariadb, sqlite, mongodb, redis, elasticsearch, clickhouse": "",
    "e2e:db": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh false",
    "e2e:db:headless": "VITE_E2E_TEST=true bash ../dev/run-e2e.sh true",
    "e2e:keyboard:postgres:headless": "VITE_E2E_TEST=true WHODB_PW_NO_DEPS=true bash ../dev/run-e2e.sh true postgres keyboard-shortcuts",
    "# EE database-specific commands - usage: pnpm e2e:ee:db [database]": "",
    "e2e:ee:db": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only false",
    "e2e:ee:db:headless": "VITE_E2E_TEST=true bash ../ee/dev/run-e2e.sh ee-only true",
    "screenshot": "VITE_E2E_TEST=true bash ../dev/run-screenshot-tests.sh",
    "screenshot:copy": "bash ../dev/copy-screenshots.sh",
    "# Auxiliary test commands": "",
    "_test:ce:setup": "bash ../dev/setup-e2e.sh ce && NODE_ENV=test vite --port 3000 &",
    "_test:ce:cleanup": "pkill -f 'vite --port 3000' || true; bash ../dev/cleanup-e2e.sh ce",
    "_test:ee:setup": "bash ../dev/setup-e2e.sh ee && VITE_BUILD_EDITION=ee NODE_ENV=test vite --port 3000 &",
    "_test:ee:cleanup": "pkill -f 'vite --port 3000' || true; bash ../dev/cleanup-e2e.sh ee",
    "# Coverage and code generation utilities": "",
    "view:coverage": "nyc report --reporter=text-summary",
    "view:coverage:ce": "nyc report --reporter=text-summary",
    "view:coverage:ee": "VITE_BUILD_EDITION=ee nyc report --reporter=text-summary",
    "view:coverage:frontend": "nyc report --reporter=html && open coverage/index.html",
    "coverage:merge": "nyc merge .nyc_output coverage/coverage-final.json",
    "coverage:clean": "rm -rf .nyc_output coverage",
    "coverage:clean:all": "bash ../dev/clean-coverage.sh",
    "view:coverage:backend": "cd ../core && go tool cover -html=coverage.out",
    "generate": "graphql-codegen --config codegen.ce.yml",
    "generate:ee": "graphql-codegen --config codegen.ee.yml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.1",
