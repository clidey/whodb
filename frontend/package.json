    "@emotion/is-prop-valid": "^1.3.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-table": "^7.7.20",
    "classnames": "^2.5.1",
    "codemirror": "^6.0.2",
    "framer-motion": "^12.23.6",
